{
    "type": "project",

    "require": {
        "composer/ca-bundle": "^1.5.10",
        "enshrined/svg-sanitize": "^0.22",
        "parsedown/parsedown": "^1.7.5",
        "parsedown/parsedown-extra": "dev-temp",
        "filp/whoops": "^2.18.4",
        "lbuchs/webauthn": "^2.2.0",
        "psr/log": "^3.0.2",
        "ramsey/http-range": "^2.0.0",
        "symfony/console": "^7.4.3",
        "symfony/http-foundation": "^7.4.3",
        "symfony/var-dumper": "^7.4.3",
        "symfony/yaml": "^7.4.1",
        "voku/anti-xss": "^4.1.42",
        "voku/portable-utf8": "dev-master as 6.0.14-dev"
    },

    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },

    "config": {
        "autoloader-suffix": "RedaxoCore",
        "bump-after-update": true,
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.3.5"
        },
        "platform-check": false,
        "prepend-autoloader": false
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gharlan/parsedown-extra"
        }
    ]
}
